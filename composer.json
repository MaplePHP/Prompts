  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Daniel Ronkainen",
      "email": "daniel.ronkainen@wazabii.se"
    },
    {
      "name": "MaplePHP",
      "homepage": "https://wazabii.se"
    }
  ],
  "require": {
    "php": ">=8.2",
    "maplephp/dto": "^3.1",
    "maplephp/http": "^2.0",
    "maplephp/validate": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "MaplePHP\\Prompts\\": "src"
    }
  },
  "minimum-stability": "dev"
